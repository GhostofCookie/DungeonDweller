/// Main tester for RoomTree
///

#include <cppunit/TextTestRunner.h>
using namespace CppUnit;

#include "newRoomTest.h"
#include "moveTest.h"
#include "atTest.h"

int main()
{
   TextTestRunner runner;
   runner.addTest(newRoomTest::suite());
   runner.addTest(moveTest::suite());
   runner.addTest(atTest::suite());
   runner.run();
   return 0;
}
